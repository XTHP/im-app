{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": [
      false
    ],
    "only-arrow-functions": [
      false
    ],
    // 取消按顺序引入
    "ordered-imports": false,
    "no-empty":false
  }
}
